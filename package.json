{
  "name": "series-tech-movies-browser",
  "version": "1.0.0",
  "description": "Custom browser for Series.tech Movies",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build-win": "electron-builder --win"
  },
  "keywords": [
    "browser",
    "electron",
    "adblocker"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.28.0",
    "cross-fetch": "^3.1.5",
    "discord-rpc": "^4.0.1"
  },
  "build": {
    "appId": "com.seriestechnmovies.browser",
    "productName": "Series.tech Movies",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}

